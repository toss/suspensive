  "devDependencies": {
    "@suspensive/eslint-config": "workspace:*",
    "@babel/core": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/react": "catalog:react19",
    "@types/react-test-renderer": "^19.0.0",
    "jest": "catalog:",
    "jest-expo": "catalog:react19",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "preset": "jest-expo"
